{
  "name": "@alyvro/api-service",
  "version": "1.0.10",
  "description": "A minimal yet powerful service for sending HTTP requests on the client and handling them gracefully on the server. No complicated setup. Just plug in your keys â€” and you're ready to go.",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "files": [
    "dist",
    "README"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./plugins": {
      "types": "./dist/plugins.d.ts",
      "import": "./dist/esm/plugins.js",
      "require": "./dist/plugins.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/esm/types.js",
      "require": "./dist/types.js"
    }
  },
  "scripts": {
    "build": "pnpm remove-dist && tsup",
    "remove-dist": "tsx scripts/remove-dist.ts",
    "rel": "pnpm build && pnpm publish",
    "test": "vitest --run",
    "coverage": "vitest run --coverage",
    "release": "release-it",
    "changelog": "release-it --only-changelog"
  },
  "keywords": [
    "api-service",
    "api",
    "alyvro-package",
    "alyvro"
  ],
  "author": "alyvro",
  "license": "MIT",
  "repository": {
    "url": "git+https://github.com/Alyvro/api-service.git"
  },
  "bugs": {
    "url": "https://github.com/Alyvro/api-service/issues",
    "email": "support@alyvro.com"
  },
  "packageManager": "pnpm@10.11.1",
  "dependencies": {
    "axios": "^1.11.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "parse-url": "^10.0.3",
    "tsup": "^8.5.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/axios": "^0.14.4",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "msw": "^2.11.3",
    "release-it": "^19.0.5",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "whatwg-fetch": "^3.6.20"
  }
}
