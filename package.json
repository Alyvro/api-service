{
  "name": "@alyvro/api-service",
  "version": "1.0.22",
  "description": "A minimal yet powerful service for sending HTTP requests on the client and handling them gracefully on the server. No complicated setup. Just plug in your keys â€” and you're ready to go.",
  "types": "./dist/index.d.mts",
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "files": [
    "dist",
    "README"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./plugins": {
      "import": "./dist/esm/plugins.js",
      "require": "./dist/plugins.js",
      "types": "./dist/plugins.d.ts"
    },
    "./types": {
      "import": "./dist/esm/types.js",
      "require": "./dist/types.js",
      "types": "./dist/types.d.ts"
    }
  },
  "scripts": {
    "build": "pnpm remove-dist && tsup",
    "remove-dist": "tsx scripts/remove-dist.ts",
    "rel": "pnpm build && pnpm publish"
  },
  "keywords": [
    "api-service",
    "api",
    "alyvro-package",
    "alyvro"
  ],
  "author": "alyvro",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/Alyvro/api-service"
  },
  "bugs": {
    "url": "https://github.com/Alyvro/api-service/issues",
    "email": "support@alyvro.com"
  },
  "packageManager": "pnpm@10.11.1",
  "dependencies": {
    "axios": "^1.11.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "tsup": "^8.5.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.1.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
